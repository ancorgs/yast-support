{
    textdomain "support";

    include "ui/common_popups.ycp";

    string filename = "/var/log/boot.msg";
    
    // title for view window, %1 will be replaced by filename
    ShowFilePopup ( sformat( _("Boot log (%1)"), filename ), filename );

    return true;
}
