/* 
 * view_messages.ycp
 *
 * small script for easy /var/log/messages viewing
 *
 * Author: Klaus Kaempf <kkaempf@suse.de>
 *
 * $Id$
 */

{
    textdomain "support";

    include "ui/common_popups.ycp";

    string filename = "/var/log/messages";
    
    // title for view window, %1 will be replaced by filename
    ShowFilePopup ( sformat( UI(_("System log (%1)") ), filename ), filename );

    return true;
}
